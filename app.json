{
  "expo": {
    "name": "Chirun",
    "slug": "chirun",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/logo-inside.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs to get your location..."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "com.maximumapex.chirun"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d3fe14a6-109e-4dd1-8330-82236716a6ba"
      }
    }
  }
}
